<!DOCTYPE html>
<html lang="{{ locale }}" dir="{{ direction }}" class="{{ checkout_html_classes }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, height=device-height, minimum-scale=1.0, user-scalable=0">
    <meta name="referrer" content="origin">

    <title>{{ page_title }}</title>

    {{ content_for_header }}

    {{ checkout_stylesheets }}
    {% render 'css_variables_checkout' %}
    <script>document.documentElement.className = document.documentElement.className.replace('no-js', 'js');</script>
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-TFMDWYM159"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'G-TFMDWYM159');
    </script>
    <!-- END Google tag (gtag.js) -->
    {{ 'fonts.css' | asset_url | stylesheet_tag: 'all', preload: 'true' }}
    {{ 'tailwind.css' | asset_url | stylesheet_tag: 'all', preload: 'true' }}
    {{ 'footer.css' | asset_url | stylesheet_tag: 'all', preload: 'true' }}
    {{ 'theme.css' | asset_url | stylesheet_tag: 'all', preload: 'true' }}
    <style>
    @media (min-width: 768px) {
      .md\:flex {
          display: flex!important;
        }
    }
    @media (min-width: 1024px) {
      .lg\:flex {
          display: flex!important;
        }
        .lg\:block {
            display: block!important;
          }
    }
    .navmenu-link.navmenu-link-depth-1{
      color:white;
    }
    .navmenu-item-text.navmenu-link-parent:hover{
      color:black;
    }
    #shopify-section-footer .center-section a:hover{
      color:white;
    }
    </style>
    {{ checkout_scripts }}
  <script src="{{ 'ascii-only.min.js' | asset_url }}" defer></script>
  
  {% render 'shogun-head' %}
</head>
  <body>
    {{ skip_to_content_link }}
    {% section 'header' %}
    <header class="banner" data-header role="banner">
      <div class="wrap">
        {{ content_for_logo }}
      </div>
    </header>

    {{ order_summary_toggle }}
    <div class="content" data-content>
      <div class="wrap">
        <div class="main">
          <header class="main__header" role="banner">
            {{ content_for_logo }}
            {{ breadcrumb }}
            {{ alternative_payment_methods }}
          </header>
          <main class="main__content" role="main">
            {{ content_for_layout }}
          </main>
          <footer class="main__footer" role="contentinfo">
            {{ content_for_footer }}
          </footer>
        </div>
        <aside class="sidebar" role="complementary">
          <div class="sidebar__header">
            {{ content_for_logo }}
          </div>
          <div class="sidebar__content">
            {{ content_for_order_summary }}
          </div>
        </aside>
      </div>
    </div>
    {% section 'footer' %}
    <script src="{{ 'accordion.js' | asset_url }}" async defer="defer"></script>
    {{ tracking_code }}
    {% comment %}Added by Ambaum for Rammount on August 11, 2022{% endcomment %}
    {% if settings.checkout_customization %}
    {% render 'checkout-customization' %}
    {% endif %}
    {%- render 'ga-checkout-events' -%}
  </body>

</html>
