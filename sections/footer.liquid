<div class="footer-container bg-black w-full p-6 px-3.5 py-10 md:p-10 border no-print">
	<div class="top-section flex lg:flex-row flex-col w-full text-white container m-auto">
		<div class="pl-0 lg:w-2/6 w-full pb-5 px-2">
			{% if section.settings.footer_logo != blank %}
				<div class="first-container mb-0 overflow-hidden flex items-center justify-center">
					<img src="{{ section.settings.footer_logo | image_url: width: 250  }}" class="object-contain h-full" />
				</div>
			{% endif %}
		</div>
		<div class="lg:w-2/6 w-full pb-5 px-2 hidden md:flex ">
			{% if section.settings.enable_newsletter %}
				<div class="flex justify-center flex-col text-center max-w-md w-full m-auto">
				{% if section.settings.newsletter_heading != blank %}<h2 class="text-[24px] mb-[16px] font-bold leading-normal tracking-[.2px] ">{{section.settings.newsletter_heading }}</h2>{% endif %}
				{% if section.settings.newsletter_copy != blank %}<span class="text-small leading-normal">{{section.settings.newsletter_copy }}</span>{% endif %}
				<strong class="relative block py-4">
					<span class="sr-only">Email Input (Newsletter)</span>
					<form action=https://rammount.us5.list-manage.com/subscribe/post?u=04510a35aa83858bd36e333f8&amp;id=107887c349 method="post" class="flex relative" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" target="_blank" novalidate="">


					<input type="hidden" name="contact[tags]" value="newsletter">
					<input
						class="!rounded input input-flat border border-slate-300 bg-white w-full p-2 newsletter text-grey"
						id="mce-EMAIL-FOOTER" type="email" name="EMAIL"
						value="{{ form.email }}" aria-required="true" autocorrect="off" autocapitalize="off"
						autocomplete="email" {% if form.errors %} autofocus aria-invalid="true"
						aria-describedby="Newsletter-error--{{ section.id }}" {% elsif form.posted_successfully? %}
						aria-describedby="Newsletter-success--{{ section.id }}" {% endif %}
						placeholder="{{ 'newsletter.label' | t }}" required>

					<input type="submit" value="Join" class="btn-ram btn-sm hover:bg-black ml-2">

					{%- if form.errors -%}
					<small id="Newsletter-error--{{ section.id }}">{{ form.errors.translated_fields['email'] |
						capitalize }} {{ form.errors.messages['email'] }}</small>
					{%- endif -%}

					{%- if form.posted_successfully? -%}
					<div class="signup-success tex-xl text-white mt-4 basis-full absolute bottom-[-60%]">Thank you for subscribing.</div>
					{%- endif -%}
					</form>

				</strong>
				</div>
			{%- endif -%}

		</div>
		<div class="pr-0 lg:w-2/6 w-full pb-5 px-2  hidden md:flex ">
			<div class="flex logo-social flex justify-center flex-col text-center">
			{%- if section.settings.show_social_icons -%}
				<h4 class="text-small">Connect With Us</h4>
				<div class="flex w-full py-4 items-center justify-center social-icons mobile">
					<ul class="flex justify-center " role="list">
						{%- if settings.social_twitter_link != blank -%}
						<li class="block py-2 px-[10px]">
							<a href="{{ settings.social_twitter_link }}" class="block w-8 h-8 opacity-75 hover:text-[#08a0e9] hover:opacity-100"
								aria-describedby="a11y-external-message">
								{%- render 'icons' icon: 'twitter' -%}
								<span class="sr-only">{{ 'general.social.links.twitter' | t }}</span>
							</a>
						</li>
						{%- endif -%}
						{%- if settings.social_facebook_link != blank -%}
						<li class="block py-2 px-[10px]">
							<a href="{{ settings.social_facebook_link }}" class="block w-8 h-8 opacity-75 hover:text-[#3b5998] hover:opacity-100"
								aria-describedby="a11y-external-message">
								{%- render 'icons' icon: 'facebook' -%}
								<span class="sr-only">{{ 'general.social.links.facebook' | t }}</span>
							</a>
						</li>
						{%- endif -%}
						{%- if settings.social_pinterest_link != blank -%}
						<li class="block py-2 px-[10px]">
							<a href="{{ settings.social_pinterest_link }}" class="block w-8 h-8 opacity-75 hover:text-[#3b5998] hover:opacity-100"
								aria-describedby="a11y-external-message">
								{%- render 'icons' icon: 'pinterest' -%}
								<span class="sr-only">{{ 'general.social.links.pinterest' | t }}</span>
							</a>
						</li>
						{%- endif -%}
						{%- if settings.social_instagram_link != blank -%}
						<li class="block py-2 px-[10px]">
							<a href="{{ settings.social_instagram_link }}" class="block insta-link relative w-8 h-8 opacity-75  hover:text-[#d6249f] hover:opacity-100"
								aria-describedby="a11y-external-message">
								{%- render 'icons' icon: 'instagram' -%}
								<span class="sr-only">{{ 'general.social.links.instagram' | t }}</span>
							</a>
						</li>
						{%- endif -%}
						{%- if settings.social_tiktok_link != blank -%}
						<li class="block py-2 px-[10px]">
							<a href="{{ settings.social_tiktok_link }}" class="block w-8 h-8 opacity-75"
								aria-describedby="a11y-external-message">
								{%- render 'icons' icon: 'tiktok' -%}
								<span class="sr-only">{{ 'general.social.links.tiktok' | t }}</span>
							</a>
						</li>
						{%- endif -%}
						{%- if settings.social_tumblr_link != blank -%}
						<li class="block py-2 px-[10px]">
							<a href="{{ settings.social_tumblr_link }}" class="block w-8 h-8 opacity-75 hover:text-[#3b5998] hover:opacity-100"
								aria-describedby="a11y-external-message">
								{%- render 'icons' icon: 'tumblr' -%}
								<span class="sr-only">{{ 'general.social.links.tumblr' | t }}</span>
							</a>
						</li>
						{%- endif -%}
						{%- if settings.social_snapchat_link != blank -%}
						<li class="block py-2 px-[10px]">
							<a href="{{ settings.social_snapchat_link }}" class="block w-8 h-8 opacity-75 hover:text-[#3b5998] hover:opacity-100"
								aria-describedby="a11y-external-message">
								{%- render 'icons' icon: 'snapchat' -%}
								<span class="sr-only">{{ 'general.social.links.snapchat' | t }}</span>
							</a>
						</li>
						{%- endif -%}
						{%- if settings.social_youtube_link != blank -%}
						<li class="block py-2 px-[10px]">
							<a href="{{ settings.social_youtube_link }}" class="block w-8 h-8 opacity-75 hover:text-red hover:opacity-100"
								aria-describedby="a11y-external-message">
								{%- render 'icons' icon: 'youtube' -%}
								<span class="sr-only">{{ 'general.social.links.youtube' | t }}</span>
							</a>
						</li>
						{%- endif -%}
						{%- if settings.social_vimeo_link != blank -%}
						<li class="block py-2 px-[10px]">
							<a href="{{ settings.social_vimeo_link }}" class="block w-8 h-8 opacity-75 hover:text-[#3b5998] hover:opacity-100"
								aria-describedby="a11y-external-message">
								{%- render 'icons' icon: 'vimeo' -%}
								<span class="sr-only">{{ 'general.social.links.vimeo' | t }}</span>
							</a>
						</li>
						{%- endif -%}
					</ul>
				</div>
				{%- endif -%}
			</div>
		</div>

	</div>
	<div class="center-section flex flex-row w-full text-white container m-auto">
		<div class="md:hidden w-full">
			{% render 'mobile-footer-accordion' %}
		</div>
			{% for block in section.blocks %}
				{% if block.type == 'column_block' %}

						<div class="hidden md:flex w-1/{{section.blocks.size}} px-2">
								<div class="flex flex-col md:flex-row text-xs mx-auto">
									<div>
										<h4 class="block font-medium mb-1 text-base">{{ block.settings.text}}</h4>
										<ul class="flex flex-col menu">
											{% if checkout %}
												{% assign menu = linklists[block.settings.column_menu].links %}
											{% else %}
												{% assign menu = block.settings.column_menu.links %}
											{% endif %}
											{% for link in menu %}
											<li class="block py-1">
												{% if link.levels == 0 %}
												<a class="text-sm font-thin transition-short border-b border-transparent hover:border-current"
													href="{{ link.url }}">{{ link.title }}</a>
												{% endif %}
												{% if link.levels > 0 %}
												<a class="text-small uppercase font-light text-white pb-2">{{ link.title }}</a>
												{% endif %}
												{% if link.levels > 0 %}
												<ul>
													{% for child_link in link.links %}
													<li class="block pt-2">
														<a class="text-small transition-short border-b border-transparent hover:border-current py-2"
															href="{{ child_link.url }}">{{ child_link.title }}</a>
													</li>
													{% endfor %}
												</ul>
												{% endif %}
											</li>
											{% endfor %}
										</ul>
									</div>
								</div>
							</div>
				{% endif %}
			{% endfor %}
	</div>

	<!-- Trademark Footer -->
		<div class="h-[30px] mt-[0px] sm:mt-[20px] text-xs text-gray sm:border-t">
			<div class="flex flex-row h-full w-full align-middle justify-center">
				<div class="flex text-xs align-middle items-center text-center pt-4  w-fulltext-gray">
					{{ section.settings.copyright_text }}

				</div>

				<div class="grid grid-cols-6  align-middle items-center text-gray hidden sm:flex pt-4 pl-2">

					{% if section.settings.show_payment_options %}
					<!-- Payment Methods -->
