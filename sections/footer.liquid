	{% schema %}
	{
	"name": "Footer",
	"class": "no-print",
	"tag": "section",
			"settings": [
			{
				"type": "checkbox",
				"id": "enable_newsletter",
				"label": "Enable Newsletter",
				"default": true
				},
				{
				"type": "richtext",
				"id": "newsletter_heading",
				"label": "Newsletter Heading",
				"default": "<p>Newsletter Heading</p>"

				},
				{
				"type": "richtext",
				"id": "newsletter_copy",
				"label": "Copy",
				"default": "<p>Newsletter Copy</p>"
				},
				{
					"type": "richtext",
					"id": "column_text",
					"label": "Right Column Text",
					"default": "<p>AMBAUM.COM - Shopify Experts</p>"
				},
				{
				"type": "richtext",
				"id": "copyright_text",
				"label": "Copyright Text",
				"default": "<p>AMBAUM.COM - Shopify Experts</p>"
				},

				{
				"type": "image_picker",
				"id": "footer_logo",
				"label": "Logo"
				},
				{
				"type": "checkbox",
				"id": "show_payment_options",
				"label": "Show Payment Options",
				"default": true
				},
				{
				"type": "checkbox",
				"id": "show_social_icons",
				"label": "Show Social Icons",
				"default": true
				},
				{
				"type": "link_list",
				"label": "Footer Menu",
				"id": "footer_menu"
				}
			],
			"blocks": [
		    {
		      "type": "column_block",
		      "name": "Column",
		      "settings": [
							{
							"type": "link_list",
							"label": "Column Menu",
							"id": "column_menu"
						},
		        {
		          "type": "richtext",
		          "id": "text",
		          "label": "Menu Heading"
		        }
		      ]
		    }
		  ],
		"presets": [
			{
			"name": "Footer"
			}
		]
	}
	{% endschema %}
