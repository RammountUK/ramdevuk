<ul class="grid grid-cols-5 {{ gap | default: 'gap-4' }} w-52" role="list">
    {%- if settings.social_twitter_link != blank -%}
        <li>
                <a href="{{ settings.social_twitter_link }}" class="block {{ size | default: 'w-8 h-8'  }}" aria-describedby="a11y-external-message">
                {%- render 'icons' icon: 'twitter' -%}
                <span class="sr-only">{{ 'general.social.links.twitter' | t }}</span>
